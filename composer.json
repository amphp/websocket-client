{
    "name": "amphp/websocket-client",
    "description": "Async WebSocket client for PHP based on Amp.",
    "license": "MIT",
    "authors": [
        {
            "name": "Bob Weinand",
            "email": "bobwei9@hotmail.com"
        },
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@trowski.com"
        },
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com"
        }
    ],
    "support": {
        "issues": "https://github.com/amphp/websocket-client/issues"
    },
    "keywords": [
        "async",
        "non-blocking",
        "websocket",
        "client",
        "http",
        "amp",
        "amphp"
    ],
    "require": {
        "php": ">=8.1",
        "amphp/amp": "^3",
        "amphp/http": "^1.3",
        "amphp/http-client": "^5",
        "amphp/socket": "^2",
        "amphp/websocket": "^2",
        "league/uri": "^6",
        "psr/http-message": "^1",
        "revolt/event-loop": "^0.2.4"
    },
    "require-dev": {
        "amphp/http-server": "^3",
        "amphp/websocket-server": "^3",
        "amphp/phpunit-util": "^3",
        "amphp/php-cs-fixer-config": "^2",
        "phpunit/phpunit": "^9",
        "psr/log": "^1",
        "psalm/phar": "^4.23"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Amp\\Websocket\\Client\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\Websocket\\Client\\Test\\": "test"
        }
    }
}
