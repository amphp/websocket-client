{
    "name": "amphp/websocket-client",
    "description": "Async WebSocket client for PHP based on Amp.",
    "license": "MIT",
    "authors": [
        {
            "name": "Bob Weinand",
            "email": "bobwei9@hotmail.com"
        },
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@trowski.com"
        },
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com"
        }
    ],
    "support": {
        "issues": "https://github.com/amphp/websocket-client/issues"
    },
    "keywords": [
        "async",
        "non-blocking",
        "websocket",
        "client",
        "http",
        "amp",
        "amphp"
    ],
    "require": {
        "php": ">=7.1",
        "amphp/amp": "^2",
        "amphp/http": "^1.1",
        "amphp/socket": "dev-master as 1.0",
        "amphp/websocket": "socket-1.0-dev as 1.0",
        "league/uri-schemes": "^1.2"
    },
    "require-dev": {
        "amphp/http-server": "v2.x-dev as 1.0",
        "amphp/websocket-server": "http-server-2.0-dev as 2.0",
        "amphp/phpunit-util": "^1.1",
        "amphp/php-cs-fixer-config": "dev-master",
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Amp\\Websocket\\Client\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\Websocket\\Client\\Test\\": "test"
        }
    },
    "config": {
        "platform": {
            "php": "7.1"
        }
    }
}
