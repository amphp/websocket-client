{
    "name": "amphp/websocket-client",
    "description": "Async WebSocket client for PHP based on Amp.",
    "license": "MIT",
    "authors": [
        {
            "name": "Bob Weinand",
            "email": "bobwei9@hotmail.com"
        },
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@trowski.com"
        },
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com"
        }
    ],
    "support": {
        "issues": "https://github.com/amphp/websocket-client/issues"
    },
    "keywords": [
        "async",
        "non-blocking",
        "websocket",
        "client",
        "http",
        "amp",
        "amphp"
    ],
    "require": {
        "php": ">=7.2",
        "amphp/amp": "^2.2",
        "amphp/http": "^1.3",
        "amphp/http-client": "^4",
        "amphp/socket": "^1",
        "amphp/websocket": "^1",
        "league/uri": "^6",
        "psr/http-message": "^1"
    },
    "require-dev": {
        "amphp/http-server": "^2",
        "amphp/websocket-server": "dev-master as 2.0-rc4",
        "amphp/phpunit-util": "^1.1",
        "amphp/php-cs-fixer-config": "dev-master",
        "phpunit/phpunit": "^8 || ^7",
        "psr/log": "^1",
        "vimeo/psalm": "^3.11@dev"
    },
    "minimum-stability": "RC",
    "autoload": {
        "psr-4": {
            "Amp\\Websocket\\Client\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\Websocket\\Client\\Test\\": "test"
        }
    }
}
